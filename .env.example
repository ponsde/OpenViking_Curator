# ---- OpenViking ----
OPENVIKING_CONFIG_FILE=/path/to/ov.conf
CURATOR_DATA_PATH=./data
CURATOR_CURATED_DIR=./curated

# ---- OAI-compatible endpoint (router/judge/answer) ----
CURATOR_OAI_BASE=https://your-oai-endpoint/v1
CURATOR_OAI_KEY=your_oai_api_key
CURATOR_ROUTER_MODELS=gemini-3-flash-preview,gemini-3-flash-high,claude-sonnet-4-5
CURATOR_JUDGE_MODEL=gemini-3-flash-preview
CURATOR_JUDGE_MODELS=gemini-3-flash-preview,gemini-3-flash-high,claude-sonnet-4-5
CURATOR_ANSWER_MODELS=gemini-3-flash-preview,gemini-3-flash-high,claude-sonnet-4-5

# ---- Grok search backend ----
CURATOR_GROK_BASE=http://127.0.0.1:8000/v1
CURATOR_GROK_KEY=your_grok_api_key
CURATOR_GROK_MODEL=grok-4-fast

# ---- Storage ----
CURATOR_FEEDBACK_FILE=feedback.json
CURATOR_CASE_DIR=cases
CURATOR_FRESHNESS_FILE=output/freshness.json
CURATOR_CAPTURE_CASE=1
